<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.juancarlos.tubbiz">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tubbiz"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Tubbiz">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Clientes.Login" />
        <activity android:name=".Clientes.Registro" />
        <activity
            android:name=".Clientes.AreaAdmin"
            android:label="@string/title_activity_area_admin"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Clientes.AreaUsuario"
            android:label="@string/title_activity_area_usuario"
            android:theme="@style/AppTheme" />
        <activity android:name=".Libros.InsertarLibro"></activity>
    </application>

</manifest>